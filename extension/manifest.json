{
	"manifest_version": 2,
	"name": "Never Late",
	"version": "1.00",
	"description": "Scans current web browser screen for due dates and saves event on Google Calendar.",
	"key": "",
	"oauth2": {
		"client_id": "87063025225-u49h62h7t5toq318lqprmp1b63dfpghc.apps.googleusercontent.com",
		"scopes":["https://www.googleapis.com/auth/calendar", "https://accounts.google.com"]
	},
	"browser_action": {
		"default_icon": "neverlatephoto.png"
	},
	"content_scripts": [
	{
		"matches": [
			"<all_urls>"
		],
		"js": ["content.js"]
	}
	],
	"background": {
		"scripts": ["background.js", "tabOpener.js"]
	},
	"permissions": [
		"identity",
		"background",
		"tabs",
		"notifications",
		"storage",
		"cookies"
	]
}
